    },
    "endHumidity": {
      "type": "number",
      "format": "double",
      "nullable": true
    },
    "endTemperature": {
      "type": "number",
      "format": "double",
      "nullable": true
    },
    "endTime": {
      "type": "integer",
      "format": "int64",
      "nullable": true
    },
