{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "StellinaSunModeOperation",
  "type": "object",
  "properties": {
    "appVersion": {
      "type": "string"
    },
    "deviceId": {
      "type": "string"
    },
    "eclipse": {
      "type": "boolean",
      "nullable": true
    },
    "endTime": {
      "type": "integer",
      "format": "int64",
      "nullable": true
    },
    "error": {
      "$ref": "./StellinaError.json"
    },
    "filterDetected": {
      "type": "boolean",
      "nullable": true
    },
    "filterDetectionSafetyTimeout": {
      "type": "boolean",
      "nullable": true
