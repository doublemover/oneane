    "observationError": {
      "$ref": "./StellinaError.json"
    },
    "safetyEnabled": {
      "type": "boolean"
    },
    "startTime": {
      "type": "integer",
      "format": "int64"
    },
    "state": {
      "$ref": "./StellinaSunModeOperation_StellinaSunModeState.json"
    },
    "stopped": {
      "type": "boolean"
    },
    "type": {
      "$ref": "./StellinaOperationType.json"
    },
    "userId": {
      "type": "integer",
      "format": "int32",
      "nullable": true
